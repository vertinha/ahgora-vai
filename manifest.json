{
    "manifest_version": 2,
    "name": "AhgoraVai",
    "version": "0.1",
    "icons": {
       "48": "icon48.png",
       "128": "icon128.png"
    },
    "page_action": {
        "default_icon": {
            "19" : "icon19.png"
        },
        "default_title": "AhgoraVai"
    },
    "permissions": [
        "https://www.ahgora.com.br/*"
    ],
    "background": {
        "scripts": [ "background.js" ],
        "persistent": false
    },
    "content_scripts": [ {
        "js": [ "jquery.js", "index.js" ],
        "matches": [ "https://www.ahgora.com.br/externo/batidas" ]
    } ]
}
